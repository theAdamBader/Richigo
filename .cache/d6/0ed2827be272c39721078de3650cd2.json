":{"line":53,"column":0},"source":"js/jquery.js","original":{"line":58,"column":34}},{"generated":{"line":53,"column":7},"source":"js/jquery.js","original":{"line":58,"column":10}},{"generated":{"line":53,"column":9},"source":"js/jquery.js","original":{"line":60,"column":13}},{"generated":{"line":53,"column":12},"source":"js/jquery.js","original":{"line":58,"column":10}},{"generated":{"line":53,"column":14},"source":"js/jquery.js","original":{"line":60,"column":18}},{"generated":{"line":53,"column":26},"source":"js/jquery.js","original":{"line":60,"column":28}},{"generated":{"line":53,"column":27},"source":"js/jquery.js","original":{"line":61,"column":11}},{"generated":{"line":53,"column":28},"source":"js/jquery.js","original":{"line":58,"column":10}},{"generated":{"line":54,"column":0},"source":"js/jquery.js","original":{"line":62,"column":7}},{"generated":{"line":55,"column":0},"source":"js/jquery.js","original":{"line":63,"column":3}},{"generated":{"line":55,"column":3},"source":"js/jquery.js","original":{"line":53,"column":2}},{"generated":{"line":56,"column":0},"source":"js/jquery.js","original":{"line":64,"column":1}},{"generated":{"line":56,"column":1},"source":"js/jquery.js","original":{"line":51,"column":0}}],"sources":{"js/jquery.js":" /* LIGHTBOX IMAGES (PHOTOGRAPHY PAGE)\r\n  *The overlay allows to find the light box ID and create the close tag to be able to close the overlay image\r\n  *The $image grab the <img> tag so that later it can be use $image.attr('src', imageFile); to find the source attribution and the image souce file\r\n  *The imageAlt finds the alt function and covert it into a caption that would then be automatically put into a <h3> tag\r\n  *When the image is clicked then it would fade in within 0.5s for a smoother transition else when you close the image, the light box would hide\r\n*/ \r\n\r\n$(document).ready(function(){ \r\n  var $overlay\t= $(\"<div id='light-box'><span class='close-img'>&times</span></div>\");\r\n  var $image\t\t= $('<img>');\r\n  var $caption\t= $('<h3></h3>');\r\n  var imageFile;\r\n  var imageAlt;\r\n\r\n  $('body').append($overlay);\t\r\n  $overlay.hide();\t\r\n\r\n  $('.flex-img').click(function(){\r\n    imageFile = $(this).attr('src');\r\n    imageAlt = $(this).attr('alt');\r\n\r\n    $overlay.append($image);\r\n    $overlay.append($caption);\r\n    $image.attr('src', imageFile);\r\n    $caption.text(imageAlt);\r\n    $overlay.fadeIn('500');\t\r\n    $image.fadeIn('500');\r\n  });\r\n  \r\n  $(\"body\").on(\"click\", \"#light-box span\", function() {\r\n    $(\"#light-box\").hide();\r\n  });\r\n});\r\n\r\n/* COOKIE BANNER \r\n  *A simple cookie banner that passes information to consumers that they would enjoy experience on a desktop that has a screen width of >= 786\r\n  *It is set to have a timer where it will fade in for 0.2s and if the consumer presses 'OK' then it will fade out for 0.2\r\n*/\r\n$(document).ready(function(){   \r\n  setTimeout(function () {\r\n      $(\"#cookieBanner\").fadeIn(200);\r\n   }, 4000);\r\n  $(\"#closeCookieBanner, .cookieBannerOK\").click(function() {\r\n      $(\"#cookieBanner\").fadeOut(200);\r\n  }); \r\n}); \r\n\r\n/* SCROLL FUNCTION \r\n  *Reference: https://www.w3schools.com/howto/howto_css_smooth_scroll.asp#section2\r\n*/\r\n$(document).ready(function(){\r\n\r\n  $(\"a\").on('click', function(event) {\r\n  \r\n      var hash = this.hash;\r\n\r\n      if (this.hash !== \"\") {\r\n          $('html, body').animate({\r\n              scrollTop: $(hash).offset().top\r\n          }, 800, function(){\r\n          });\r\n      }\r\n  });\r\n});"},"lineCount":null}},"error":null,"hash":"d433dca4b7743a7b020d2558ab20812f","cacheData":{"env":{}}}