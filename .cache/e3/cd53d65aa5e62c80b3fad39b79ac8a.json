olumn":4}},{"source":"sass\\page-style.scss","name":null,"original":{"line":311,"column":0},"generated":{"line":290,"column":0}},{"source":"sass\\page-style.scss","name":null,"original":{"line":312,"column":2},"generated":{"line":291,"column":2}},{"source":"sass\\page-style.scss","name":null,"original":{"line":313,"column":6},"generated":{"line":292,"column":4}},{"source":"sass\\page-style.scss","name":null,"original":{"line":314,"column":6},"generated":{"line":293,"column":4}},{"source":"sass\\page-style.scss","name":null,"original":{"line":316,"column":2},"generated":{"line":295,"column":2}},{"source":"sass\\page-style.scss","name":null,"original":{"line":317,"column":6},"generated":{"line":296,"column":4}},{"source":"sass\\page-style.scss","name":null,"original":{"line":318,"column":6},"generated":{"line":297,"column":4}},{"source":"sass\\page-style.scss","name":null,"original":{"line":320,"column":2},"generated":{"line":299,"column":2}},{"source":"sass\\page-style.scss","name":null,"original":{"line":321,"column":6},"generated":{"line":300,"column":4}},{"source":"sass\\page-style.scss","name":null,"original":{"line":322,"column":6},"generated":{"line":301,"column":4}}],"sources":{"sass\\page-style.scss":"body\r\n{\r\n  margin: 0;\r\n\tpadding: 0;\r\n  background-color: #ffffff;\r\n  overflow-x: hidden;\r\n}\r\n\r\n.container\r\n{\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.text-box\r\n{\r\n  padding-top: 50px;\r\n  padding-bottom: 100px;\r\n}\r\n\r\n.title\r\n{\r\n  color: #effd56;\r\n  font-size: 2.5em;\r\n  position: absolute;\r\n  top: 32px;\r\n  left: 20px;\r\n  margin: 0;\r\n}\r\n\r\n.headline\r\n{\r\n  font-size: 3em;\r\n  text-align: center;\r\n}\r\n\r\n.sub-title\r\n{\r\n  font-size: 1.8em;\r\n  height: auto;\r\n  width: 60%;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.paragraph-container\r\n{\r\n  height: auto;\r\n  width: 60%;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  line-height: 1.5em;\r\n}\r\n\r\n.top-page\r\n{\r\n  display: block;\r\n  height: 100%;  \r\n  width: 100%; \r\n  position: relative;\r\n  background-color: #effd56;\r\n  top: 0;\r\n  left: -10px;\r\n  padding: 60px;\r\n  z-index: -1;\r\n  transform:rotate(-1deg);\r\n}\r\n\r\nh1,\r\nh2,\r\nh3,\r\np\r\n{\r\n\ttext-transform: uppercase;\r\n  font-family: 'Quicksand', sans-serif;\r\n}\r\n\r\np{color: #000000;}\r\n\r\n.strip\r\n{\r\n    position: absolute;\r\n    height: 75%;\r\n    width: 100%;\r\n    top: 0;\r\n    left: 0;\r\n    background-color: #51a866;\r\n    padding: 10px;\r\n    transform:rotate(1deg);\r\n}\r\n  \r\n/* ================================================================================================ */\r\n\r\n  .close {\r\n    position: absolute;\r\n    right: 32px;\r\n    top: 32px;\r\n    width: 32px;\r\n    height: 32px;\r\n    opacity: 0.5;\r\n  }\r\n  .close:hover {\r\n    opacity: 1;\r\n  }\r\n  .close:before, .close:after\r\n  {\r\n    position: absolute;\r\n    left: 15px;\r\n    content: ' ';\r\n    height: 33px;\r\n    width: 2px;\r\n    background-color: #effd56;\r\n  }\r\n  .close:before {\r\n    transform: rotate(45deg);\r\n  }\r\n  .close:after {\r\n    transform: rotate(-45deg);\r\n  } \r\n\r\n/* ================================================================================================ */\r\n\r\n/* 3D Image Slider */\r\n\r\n.img-container \r\n{\r\n  width: 83%;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n}\r\n  \r\n.img-size\r\n{\r\n  height: auto;\r\n  width: 100%;\r\n  margin: auto;\r\n}\r\n\r\n/* Next & previous buttons */\r\n.prev, .next {\r\n  cursor: pointer;\r\n  position: absolute;\r\n  width: auto;\r\n  padding: 16px;\r\n  color: #000000;\r\n  font-weight: bold;\r\n  font-size: 28px;\r\n  transition: 0.6s ease;\r\n  align-items: center;\r\n}\r\n\r\n/* Position the \"next button\" to the right */\r\n.next {\r\n  right: 100px;\r\n  border-radius: 50%;\r\n}\r\n\r\n.prev {\r\n  left: 100px;\r\n  border-radius: 50%;\r\n}\r\n\r\n/* On hover, add a black background color with a little bit see-through */\r\n.prev:hover, .next:hover {\r\n  background-color: rgba(0,0,0,0.8);\r\n  color: #2fff05;\r\n}\r\n\r\n.button--inactive \r\n{\r\n  opacity: 0.2;\r\n}\r\n\r\n.cards-wrapper \r\n{\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.cards-container {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  position: relative;\r\n}\r\n  \r\n.box {\r\n  width: 100%;\r\n  height: auto;\r\n  box-shadow: 0px 0px 2rem 0px #888888;\r\n  background-color: white;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  position: relative;\r\n}\r\n\r\n.box:nth-child(2n) {\r\n  transform: scale(0.85);\r\n  z-index: -1;\r\n}\r\n\r\n.box:nth-child(2) {\r\nleft: 8%;\r\n}\r\n\r\n.box:nth-child(2n + 1) {\r\n  transform: scale(0.75);\r\n  z-index: -2;\r\n}\r\n\r\n.box:nth-child(1) {\r\nleft: 15%;\r\n}\r\n\r\n.hide-box {\r\n  display: none;\r\n}\r\n\r\n@media screen and (max-width: 800px)\r\n{\r\n  .next \r\n  {\r\n    right: 0;\r\n  }\r\n  \r\n  .prev \r\n  {\r\n    left: 0;\r\n  }\r\n\r\n  .prev:hover, .next:hover \r\n  {\r\n    background-color: white;\r\n    color: black;\r\n  }\r\n\r\n  .cards-container \r\n  {\r\n    height: 20rem;\r\n  }\r\n}\r\n\r\n.move-out-from-left {\r\n  animation: moveOutLeft 0.5s ease-in-out;\r\n}\r\n\r\n.move-out-from-right {\r\n  animation: moveOutRight 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position5-from-left {\r\n  animation: moveToP5Left 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position4-from-left {\r\n  animation: moveToP4Left 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position3-from-left {\r\n  animation: moveToP3Left 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position2-from-left {\r\n  animation: moveToP2Left 0.5s ease-in-out;\r\n}\r\n\r\n\r\n.move-to-position1-from-left{\r\n  animation: moveToP1Left 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position5-from-right{\r\n  animation: moveToP5Right 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position4-from-right{\r\n  animation: moveToP4Right 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position3-from-right{\r\n  animation: moveToP3Right 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position2-from-right{\r\n  animation: moveToP2Right 0.5s ease-in-out;\r\n}\r\n\r\n.move-to-position1-from-right{\r\n  animation: moveToP1Right 0.5s ease-in-out;\r\n}\r\n  \r\n@keyframes moveOutLeft {\r\n  0% {\r\n      transform: scale(0.75) translateX(0%);\r\n      opacity: 1;\r\n  }\r\n  50% {\r\n      transform: scale(0.5) translateX(-150%);\r\n      opacity: 0.5;\r\n  }\r\n  100% {\r\n      transform: scale(0.25) translateX(0%);\r\n      opacity: 0;\r\n  }\r\n}\r\n\r\n\r\n@keyframes moveOutRight {\r\n  0% {\r\n      transform: scale(0.75) translateX(0%);\r\n      opacity: 1;\r\n  }\r\n  50% {\r\n      transform: scale(0.5) translateX(150%);\r\n      opacity: 0.5;\r\n  }\r\n  100% {\r\n      transform: scale(0.25) translateX(0%);\r\n      opacity: 0;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"b735b92dbbd280c5394d63a2208cc6a8","cacheData":{"env":{}}}