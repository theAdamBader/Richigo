==========================//\n\n/* NAVIGATION */\n\n#nav-grid\n{\n\tmargin-top: 100px;\n\tdisplay: grid;\n\tgrid-template-columns: repeat(2, 1fr);\n\tgrid-gap: 10px;\n\tgrid-auto-rows: minmax(100px, auto);\n\toverflow: hidden;\n}\n\nnav li\n{\n\theight: 75%;\n\tlist-style: none;\n}\n\nnav ul:hover li a\n{\n\topacity: 0.5;\n}\n\nnav ul li a:hover\n{\n\topacity: 1;\n\tcolor: #c00606;\n}\n\n.nav-linkz\n{\n\ttext-decoration: none;\n    color: #000000;\n}\n\n.nav-music\n{\n\twidth: 40%;\n\tmargin-left: 20%;\n\tfont-family: 'Arial', sans-serif;\n\tfont-size: 50px;\n\tanimation-name: music;\n\tanimation-duration: 2s;\n}\n\n@keyframes music\n{\n\tfrom{ transform: translateX(-500px); opacity: 0; }\n\tto{transform: translateX(0); opacity: 1;}\n}\n\n.nav-artwork\n{\n\twidth: 70%;\n\tmargin-left: 20%;\n\tfont-family: 'Arial', sans-serif;\n\tfont-size: 50px;\n\tanimation-name: artwork;\n\tanimation-duration: 2s;\n}\n\n@keyframes artwork\n{\n\tfrom{ transform: translateY(-400px); opacity: 0; }\n\tto{transform: translateY(0); opacity: 1;}\n}\n\n.nav-design\n{\n\tmargin-top: 50%;\n\tmargin-left: 20%;\n\tfont-family: 'Arial', sans-serif;\n\tfont-size: 50px;\n\tanimation-name: design;\n\tanimation-duration: 2s;\n}\n\n@keyframes design\n{\n\tfrom{transform: translateY(400px); opacity: 0;}\n\tto{transform: translateX(0); opacity: 1;}\n}\n\n.nav-photo\n{\n\tmargin-top: 50%;\n\tmargin-left: 20%;\n\tfont-family: 'Arial', sans-serif;\n\tfont-size: 50px;\n\tanimation-name: photo;\n\tanimation-duration: 2s;\n}\n\n@keyframes photo\n{\n\tfrom{ transform: translateX(500px); opacity: 0; }\n\tto{transform: translateX(0); opacity: 1;}\n}\n\n.red-alert:hover\n{\n\tfont-size: 60px;\n\tanimation: shake 0.3s;\n\tanimation-iteration-count: infinite;\n}\n\n@keyframes shake \n{\n\t0% {\n\t  transform: translate(1px, 1px) rotate(0deg);\n\t}\n\t10% {\n\t  transform: translate(-1px, -2px) rotate(-1deg);\n\t}\n\t20% {\n\t  transform: translate(-3px, 0px) rotate(1deg);\n\t}\n\t30% {\n\t  transform: translate(3px, 2px) rotate(0deg);\n\t}\n\t40% {\n\t  transform: translate(1px, -1px) rotate(1deg);\n\t}\n\t50% {\n\t  transform: translate(-1px, 2px) rotate(-1deg);\n\t}\n\t60% {\n\t  transform: translate(-3px, 1px) rotate(0deg);\n\t}\n\t70% {\n\t  transform: translate(3px, 1px) rotate(-1deg);\n\t}\n\t80% {\n\t  transform: translate(-1px, -1px) rotate(1deg);\n\t}\n\t90% {\n\t  transform: translate(1px, 2px) rotate(0deg);\n\t}\n\t100% {\n\t  transform: translate(1px, -2px) rotate(-1deg);\n\t}\n}\n\n.centreImg\n{\n\tdisplay: block;\n\theight: 100%;\n\twidth: 100%;\n\tposition: absolute;\n\tleft:0; \n\tright:0;\t\n\ttop:0; \n\tbottom:0;\n\tz-index: 98;\n}\n\n.aboutClick\n{\n\twidth: 100%;\n\theight: 50%;\t\n\tposition: absolute;\n\tbottom: 0;\n\ttext-align: center;\n\tmargin-left: auto;\n\tmargin-right: auto;\n\tz-index: 99;\n}\n\nbutton\n{\n\tborder: 0;\n\tpadding: 0;\n\tbackground: none;\n\tfont-size: 2em;\n\ttext-decoration: none; \n\tcursor: pointer;\n}\n\n//=================================================================================================================//\n\n/* SPOTLIGHT */\n.focus\n{\n\tposition: absolute;\n\tdisplay: block;\n\twidth: 100%;\n\theight: 100%;\n\ttop: 0;\n\ttransform: translateY(100vh);\n\tbackground: radial-gradient(circle at var(--x, 50%) var(--y, 50%), rgba(#000000, 10%) 14%, #000000 18%);\n\tpointer-events: none;\n\tmix-blend-mode: darken;  \n\tz-index: -2;\n}\n\n/* ================================================================================================ */\n\n/* CLOSE BUTTON */\n.close \n{\n\tposition: absolute;\n\tright: 32px;\n\ttop: 32px;\n\twidth: 32px;\n\theight: 32px;\n\topacity: 0.5;\n}\n\n.close:hover \n{\n\topacity: 1;\n}\n\n.close:before, .close:after \n{\n\tposition: absolute;\n\tleft: 15px;\n\tcontent: ' ';\n\theight: 33px;\n\twidth: 2px;\n\tbackground-color: rgb(27, 31, 255);\n}\n\n.close:before\n{\n\ttransform: rotate(45deg);\n}\n\n.close:after \n{\n\ttransform: rotate(-45deg);\n} \n\n//=================================================================================================================//\n\n/* MOBILE VERSION */\n\n@media screen and (max-width: 800px)\n{\n\n\t.mobpage{display: block;}\n\t.mainpage{display: none;}\n\n\t.focus\n\t{\n\t\tdisplay: none;\n\t}\n\n\tnav\n\t{\n\t\ta{font-size: x-large;}\n\t\tul:hover li a{opacity: 1;}\n\t\tul li a:hover{color: #000000;}\n\t}\n\t\n\t.row \n\t{\n\t\twidth: 90%;\n\t\ttext-align: center;\n\t}\n\n\t.grid-container\n\t {\n\t\tdisplay: inline-flex;\n\t\tflex-flow: column wrap;\n\t\tpadding: 10px;\n\t\tlist-style: none;\n\t}\n\n\t.centreMobileImg\n\t{\n\t\tdisplay: block;\n\t\theight: auto;\n\t\twidth: 100%;\n\t\tmax-width: 50%;\n\t\tposition: relative;\n\t\tleft:0; \n\t\tright:0;\t\n\t\ttop:0; \n\t\tbottom:0;\n\t\tmargin-top: 50px;\n\t\tmargin-right:auto;\n\t\tmargin-left: auto;\n\t\tz-index: 3;\n\t\tborder-radius: 100%;\n\t}\n}"},"lineCount":null}},"error":null,"hash":"c16bb177e0791ea3df30fc8964a42ef8","cacheData":{"env":{}}}