ames artwork\n{\n\tfrom{ transform: translateY(-400px); opacity: 0; }\n\tto{transform: translateY(0); opacity: 1;}\n}\n\n.nav-design\n{\n\tmargin-top: 50%;\n\tmargin-left: 20%;\n\tfont-family: 'Arial', sans-serif;\n\tfont-size: 50px;\n\tanimation-name: design;\n\tanimation-duration: 2s;\n}\n\n@keyframes design\n{\n\tfrom{transform: translateY(400px); opacity: 0;}\n\tto{transform: translateX(0); opacity: 1;}\n}\n\n.nav-photo\n{\n\tmargin-top: 50%;\n\tmargin-left: 20%;\n\tfont-family: 'Arial', sans-serif;\n\tfont-size: 50px;\n\tanimation-name: photo;\n\tanimation-duration: 2s;\n}\n\n@keyframes photo\n{\n\tfrom{ transform: translateX(500px); opacity: 0; }\n\tto{transform: translateX(0); opacity: 1;}\n}\n\n.red-alert:hover\n{\n\tfont-size: 60px;\n\tanimation: shake 0.3s;\n\tanimation-iteration-count: infinite;\n}\n\n@keyframes shake \n{\n\t0% {\n\t  transform: translate(1px, 1px) rotate(0deg);\n\t}\n\t10% {\n\t  transform: translate(-1px, -2px) rotate(-1deg);\n\t}\n\t20% {\n\t  transform: translate(-3px, 0px) rotate(1deg);\n\t}\n\t30% {\n\t  transform: translate(3px, 2px) rotate(0deg);\n\t}\n\t40% {\n\t  transform: translate(1px, -1px) rotate(1deg);\n\t}\n\t50% {\n\t  transform: translate(-1px, 2px) rotate(-1deg);\n\t}\n\t60% {\n\t  transform: translate(-3px, 1px) rotate(0deg);\n\t}\n\t70% {\n\t  transform: translate(3px, 1px) rotate(-1deg);\n\t}\n\t80% {\n\t  transform: translate(-1px, -1px) rotate(1deg);\n\t}\n\t90% {\n\t  transform: translate(1px, 2px) rotate(0deg);\n\t}\n\t100% {\n\t  transform: translate(1px, -2px) rotate(-1deg);\n\t}\n}\n\n//=================================================================================================================//\n\n.focus\n{\n\tposition: absolute;\n\tdisplay: block;\n\twidth: 100%;\n\theight: 100%;\n\ttop: 0;\n\ttransform: translateY(100vh);\n\tbackground: radial-gradient(circle at var(--x, 50%) var(--y, 50%), rgba(#000000, 10%) 14%, #000000 18%);\n\tpointer-events: none;\n\tmix-blend-mode: darken;  \n\tz-index: -2;\n}\n//solve issue with modal\n.centreImg\n{\n\tdisplay: block;\n\theight: auto;\n\twidth: 100%;\n\tmax-width: 25%;\n\tposition: absolute;\n\tleft:0; \n\tright:0;\t\n\ttop:0; \n\tbottom:0;\n\tmargin:auto;\n\tz-index: -3;\n\tborder-radius: 100%;\n}\n\n@media screen and (max-width: 800px)\n{\n\n\t.mobpage{display: block;}\n\t.mainpage{display: none;}\n\n\t.focus\n\t{\n\t\tdisplay: none;\n\t}\n\n\tnav\n\t{\n\t\ta{font-size: x-large;}\n\t\tul:hover li a{opacity: 1;}\n\t\tul li a:hover{color: #000000;}\n\t}\n\t\n\t.row \n\t{\n\t\twidth: 90%;\n\t\ttext-align: center;\n\t}\n\n\t.grid-container\n\t {\n\t\tdisplay: inline-flex;\n\t\tflex-flow: column wrap;\n\t\tpadding: 10px;\n\t\tlist-style: none;\n\t}\n\n\t.centreMobileImg\n\t{\n\t\tdisplay: block;\n\t\theight: auto;\n\t\twidth: 100%;\n\t\tmax-width: 50%;\n\t\tposition: relative;\n\t\tleft:0; \n\t\tright:0;\t\n\t\ttop:0; \n\t\tbottom:0;\n\t\tmargin-top: 50px;\n\t\tmargin-right:auto;\n\t\tmargin-left: auto;\n\t\tz-index: 3;\n\t\tborder-radius: 100%;\n\t}\n}\n\n// @mixin grayscale_element($value) \n// {\n//     -webkit-filter: #{\"grayscale(#{$value})\"};\n//     -moz-filter: #{\"grayscale(#{$value})\"};\n//     filter:#{\"grayscale(#{$value})\"};\n// }\n\n// .grey-zone\n// {\n// \t@include grayscale_element(100%);\n//     transition: all 200ms ease-in-out;\n//     &:hover {\n//         @include grayscale_element(0);\n//     }\n// }\n\n// .grey-zone:hover\n// {\n// \tfilter: none;\n// }"},"lineCount":null}},"error":null,"hash":"619249e51b72709b7dcc6679d07483f0","cacheData":{"env":{}}}